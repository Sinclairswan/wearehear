<div class="cf ph2-ns">
   <div class="rand-song fl w-100 w-25-ns  mr2 ">
   <iframe src="https://open.spotify.com/embed?uri=spotify:track:<%=@songs.sample.spotify_id %>"
                frameborder="0" allowtransparency="true" width="300" height="380"></iframe>
   </div>

  <div class="fl w-100 w-40-ns pa2 ma2" id="mapbase"></div>

  <article class="fl w-100 w-25-ns  black-60 tr  " data-name="slab-stat">
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml0">Bouquets sent</dd>
        <dd class="f4 f2-ns b ml0"><%= @bouquets.count %></dd>
      </dl><br>
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml1">Top 5 locations</dd>
        <% @locations.each do |location| %>
        <dd class="f3 f2-ns b ml1"><%= location.reverse_geocode.split(",")[1] %></dd>
        <% end %>
      </dl>
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml1">Top 5 Songs</dd>
        <% @songs.each do |song| %>
        <dd class="f3 f2-ns b ml1"><%= song.artist %></dd>
        <% end %>
      </dl>
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml0">Days Left</dd>
        <dd class="f3 f2-ns b ml0">2</dd>
      </dl><br>
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml0">Favorite Cat</dd>
        <dd class="f3 f2-ns b ml0">All of Them</dd>
      </dl><br>
      <dl class="dib mr0">
        <dd class="f6 f5-ns b ml0">App Downloads</dd>
        <dd class="f3 f2-ns b ml0">0</dd>
      </dl>
</article>
</div>
